
<div class="container">
<h1>SignUp</h1>

<form ng-submit="signUp()">
  <div class="form-group">
    <label>First Name
      <input type="first_name" class="form-control" ng-model="user.first_name">
    </label>
  </div>
  <div class="form-group">
    <label>Last Name
      <input type="last_name" class="form-control" ng-model="user.last_name">
    </label>
  </div>
  <div class="form-group">
    <label>Email Address
      <input type="email" class="form-control" ng-model="user.email">
    </label>
  </div>
  <div class="form-group">
    <label>Password
      <input type="password" class="form-control" ng-model="user.password">
    </label>
  </div>
  <div class="form-group">
    <label>Password confirmation
      <input type="password" class="form-control" ng-model="user.password_confirmation"
    </label>
  </div>

  <input type="submit" class="btn btn-success" value="submit">
</form>
</div>
